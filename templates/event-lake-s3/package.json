{
  "name": "subsys1-event-lake-s3",
  "private": true,
  "scripts": {
    "test": "sls package -r us-west-2 -s stg",
    "test:ci": "eval \"$(assume-role) npm test\"",
    "dp:stg:e": "sls deploy --verbose -r us-east-1 -s stg --force",
    "dp:stg:w": "sls deploy --verbose -r us-west-2 -s stg --force",
    "dp:prd:e": "sls deploy --verbose -r us-east-1 -s prd --force",
    "dp:prd:w": "sls deploy --verbose -r us-west-2 -s prd --force",
    "dp:stg:e:ci": "eval \"$(assume-role) npm run dp:stg:e\"",
    "dp:stg:w:ci": "eval \"$(assume-role) npm run dp:stg:w\"",
    "dp:prd:e:ci": "eval \"$(assume-role) npm run dp:prd:e\"",
    "dp:prd:w:ci": "eval \"$(assume-role) npm run dp:prd:w\""
  },
  "devDependencies": {
    "aws-assume-role-cicd": "^1.0.0",
    "find-up": "^3.0.0",
    "serverless": "^3.0.0"
  },
  "dependencies": {
    "aws-lambda-stream": "^0.18.0"
  }
}
